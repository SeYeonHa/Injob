<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.injob.detail.mapper.DetailMapper">

	<select id="getDetailList">
		SELECT COM_ID
			,PO_ID
			,PO_GRADE
			,HOPE_DEPARTMENT
			,PO_START_DATE
			,PO_END_DATE
			,PO_PROFILE
			,PO_TITLE
			,PO_CONTENT
			,CAREER
		FROM POSTING
	</select>

</mapper>
